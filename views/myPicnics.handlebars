{{!-- {{log}} --}}
{{!-- user's picnic events overview here --}}
<section>
  <div class="my-picnics">
    {{log picnics}}
    {{#each picnics}}
    <div class="card mb-3 col-2">
      <img src="/assets/images/picnic cards (vertical)/{{verticalPhoto}}" class="card-img-top img-fluid picnic-card-img" alt="picnic imagery">
      <div class="card-body">
        <h5 class="card-title">{{event_name}}</h5>
        <p class="card-text">On {{format_time start_time}}</p>
        <p class="card-text"><small>{{address}}</small></p>
      </div>
    </div>
    {{/each}}
    {{!-- creates a picnic event card for each picnic --}}
    {{!-- {{#each picnics}}
    <div class="card text-bg-dark mb-3">
      <img src="/assets/images/picnic cards (vertical)/{{verticalPhoto}}" class="card-img" alt="picnic imagery">
      <div class="card-img-overlay">
        <h5 class="card-title">{{event_name}}</h5>
        <p class="card-text">On {{format_time start_time}}</p>
        <p class="card-text"><small>At {{address}}</small></p>
      </div>
    </div>
    {{/each}} --}}
  </div>
</section>
